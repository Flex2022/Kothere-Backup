<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="flex_additions_deductions_view_company_form" model="ir.ui.view">
            <field name="name">flex_additions_deductions.ModelName.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='currency_id']" position="after">
                        <field name="flex_additions_deductions_ids"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
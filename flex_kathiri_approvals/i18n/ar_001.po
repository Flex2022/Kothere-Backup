#. module: flex_kathiri_approvals
#: model:mail.template,body_html:flex_kathiri_approvals.mail_template_air_ticket_approval
msgid ""
"<p>Hello <t t-out=\"object.partner_id.name\"></t>,</p>\n"
"            <p>Your air ticket request has been\n"
"                <t t-if=\"object.state == 'approved'\">approved</t>\n"
"                <t t-else=\"object.state == 'rejected'\">rejected</t>\n"
"            </p>\n"
"            <p>Details:</p>\n"
"            <p>AIR Ticket Request Number:\n"
"                <t t-out=\"object.name\"></t>\n"
"            </p>\n"
"            <p>Date From:\n"
"                <t t-out=\"object.date_from\"></t>\n"
"            </p>\n"
"            <p>Date To:\n"
"                <t t-out=\"object.date_to\"></t>\n"
"            </p>\n"
"            <p>Departure City:\n"
"                <t t-out=\"object.departure_city\"></t>\n"
"            </p>\n"
"            <p>Destination City:\n"
"                <t t-out=\"object.destination_city\"></t>\n"
"            </p>\n"
"        "
msgstr ""
"<p>مرحبَا <t t-out=\"object.partner_id.name\"></t>,</p>\n"
"            <p>طلب حجز تذكرتك قد تم\n"
"                <t t-if=\"object.state == 'approved'\">الموافقة عليه</t>\n"
"                <t t-else=\"object.state == 'rejected'\">رفضه</t>\n"
"            </p>\n"
"            <p>التفاصيل:</p>\n"
"            <p>رقم طلب حجز التذكرة الجوية:\n"
"                <t t-out=\"object.name\"></t>\n"
"            </p>\n"
"            <p>من التاريخ:\n"
"                <t t-out=\"object.date_from\"></t>\n"
"            </p>\n"
"            <p>إلى التاريخ:\n"
"                <t t-out=\"object.date_to\"></t>\n"
"            </p>\n"
"            <p>مدينة المغادرة:\n"
"                <t t-out=\"object.departure_city\"></t>\n"
"            </p>\n"
"            <p>مدينة الوجهة:\n"
"                <t t-out=\"object.destination_city\"></t>\n"
"            </p>\n"
"        "


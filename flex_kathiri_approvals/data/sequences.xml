<odoo>
    <data noupdate="1">
        <!-- Define a sequence for employee transfer approvals -->
        <record id="sequence_flex_approval_employee_transfer" model="ir.sequence">
            <field name="name">Employee Transfer Approval Sequence</field>
            <field name="code">flex.approval.employee_transfer</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="prefix">ET/</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Renew Iqama Approvals -->
        <record id="sequence_flex_approval_renew_iqama" model="ir.sequence">
            <field name="name">flex.approval.renew_iqama</field>
            <field name="code">flex.approval.renew_iqama</field>
            <field name="prefix">RENEW-IQAMA/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Business Trip Approvals -->
        <record id="sequence_approval_business_trip" model="ir.sequence">
            <field name="name">flex.approval.business.trip.sequence</field>
            <field name="code">flex.approval.business_trip.sequence</field>
            <field name="prefix">BUS_TRIP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Resignation Approval Sequence -->
        <record id="sequence_resignation_approval" model="ir.sequence">
            <field name="name">Resignation Approval Sequence</field>
            <field name="code">flex.approval.resignation</field>
            <field name="padding">4</field>
            <field name="prefix">RESIGN/</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Medical Insurance Renewal Approval -->
        <record id="seq_approval_renew_medical_insurance" model="ir.sequence">
            <field name="name">Medical Insurance Renewal Approval</field>
            <field name="code">flex.approval.renew_medical_insurance</field>
            <field name="prefix">MI/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_exit_return_visa" model="ir.sequence">
            <field name="name">Exit Return Visa</field>
            <field name="code">exit.return.visa</field>
            <field name="prefix">ERV/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
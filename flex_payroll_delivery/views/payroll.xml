<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="view_hr_payslip_form_inherit_flex_payroll_delivery" model="ir.ui.view">
        <field name="name">view.hr.payslip.form.inherit.flex_payroll_delivery</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='struct_id']" position="before">
                <field name="distance_per_km_order"/>
            </xpath>

        </field>
    </record>
</odoo>